<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE TEMPLATECOLLECTION [

<!ELEMENT TEMPLATECOLLECTION (TEMPLATE)*>

<!ELEMENT TEMPLATE (FILE_NAME, DEPENDENCIES, DEPENDENCY_COUNT, THIRD_PARTY_DEPENDENCIES, DESCRIPTION, FEED_URL, IMPORTS, ARGS)>

<!ATTLIST TEMPLATE 

NAME CDATA #REQUIRED

>

<!ELEMENT FILE_NAME (#PCDATA)>

<!ELEMENT DEPENDENCIES (DEPENDENCY)*>

<!ELEMENT DEPENDENCY (#PCDATA)>

<!ELEMENT DEPENDENCY_COUNT (#PCDATA)>

<!ELEMENT THIRD_PARTY_DEPENDENCIES (EXT_DEPENDENCY)*>

<!ATTLIST THIRD_PARTY_DEPENDENCIES 

